{
  "root": true,
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "extends": ["@nuxtjs", "plugin:vue/vue3-strongly-recommended", "prettier"],
  "plugins": ["prettier"],
  "overrides": [
    {
      "files": ["pages/**/*.vue"],
      "rules": {
        "vue/multi-word-component-names": "off"
      }
    }
  ]
}
